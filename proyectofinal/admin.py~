from django.contrib import admin
from proyectofinal.models import Us
from proyectofinal.models import Tw


class PostUser(admin.ModelAdmin):
    list_display= ["usuario","img","nombre"]
    list_display_links= ["nombre"]
    list_filter= ["nombre"]
    search_fields= ["nombre"]
    class Meta:
        model = Us

class PostTwitt(admin.ModelAdmin):
    list_display= ["id_tw","tw","fec"]
    list_display_links= ["fec"]
    list_filter= ["id_tw","fec"]
    list_editable=["tw"]
    search_fields= ["id_tw"]
    class Meta:
        model = Tw

admin.site.register(Us)
admin.site.register(Tw)
